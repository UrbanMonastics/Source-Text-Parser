{
    "name": "urbanmonastics/sourceparser",
    "description": "A Fast Markdown Parser extended for Liturgy in PHP.",
    "keywords": ["markdown", "parser", "liturgy", "bible", "scripture"],
    "homepage": "http://developers.urbanmonastic.org/sourceparser/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Prins",
            "role": "Developer",
            "homepage": "https://paul.build/",
            "email": "paulprins@gmail.com"
        },
        {
            "name": "Urban Monastics",
            "homepage": "https://developers.urbanmonastic.org/"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8.35"
    },
    "autoload": {
        "psr-4": {"UrbanMonastics\\SourceParser\\": ""},
        "exclude-from-classmap": [
            "/docker/",
            "/test/"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "TestSourceParser": "test/",
            "SourceParserTest": "test/",
            "CommonMarkTest": "test/",
            "CommonMarkTestWeak": "test/"
        }
    }
}
